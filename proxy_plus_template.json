{
    "kind": "List",
    "creationTimestamp": null,
    "apiVersion": "v1beta1",
    "items": [
        {
            "kind": "Service",
            "id": "${APPNAME}proxy",
            "creationTimestamp": null,
            "apiVersion": "v1beta1",
            "port": 8080,
            "portName": "${APPNAME}proxy-tcp-8080",
            "protocol": "TCP",
            "containerPort": 8080,
            "selector": {
                "deploymentconfig": "${APPNAME}proxy"
            },
            "ports": [
                {
                    "name": "${APPNAME}proxy-tcp-8080",
                    "protocol": "TCP",
                    "port": 8080,
                    "containerPort": 8080
                }
            ]
        },
        {
            "kind": "Route",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "${APPNAME}proxy"
            },
            "host": "${APPNAME}.${APP_ROOT_URL}",
            "serviceName": "${APPNAME}proxy"
        },
        {
            "kind": "DeploymentConfig",
            "apiVersion": "v1beta1",
            "metadata": {
                "name": "${APPNAME}proxy",
                "creationTimestamp": null
            },
            "triggers": [
                {
                    "type": "ConfigChange"
                }
            ],
            "template": {
                "strategy": {
                    "type": "Recreate"
                },
                "controllerTemplate": {
                    "replicas": 1,
                    "replicaSelector": {
                        "deploymentconfig": "${APPNAME}proxy"
                    },
                    "podTemplate": {
                        "desiredState": {
                            "manifest": {
                                "version": "v1beta2",
                                "id": "",
                                "volumes": null,
                                "containers": [
                                    {
                                        "name": "${APPNAME}proxy",
                                        "image": "ryanj/hexboard:latest",
                                        "livenessProbe": {
                                            "httpGet": {
                                                "path": "/status",
                                                "port": 8080
                                            },
                                            "initialDelaySeconds": 30,
                                            "timeoutSeconds": 1
                                        },
                                        "env": [
                                            {
                                                "name": "APPNAME",
                                                "value": "${APPNAME}"
                                            },
                                            {
                                                "name": "NAMESPACE",
                                                "value": "${NAMESPACE}"
                                            },
                                            {
                                                "name": "APP_ROOT_URL",
                                                "value": "${APP_ROOT_URL}"
                                            },
                                            {
                                                "name": "PROXY",
                                                "value": "${PROXY}"
                                            },
                                            {
                                                "name": "OPENSHIFT_SERVER",
                                                "value": "${OPENSHIFT_SERVER}"
                                            },
                                            {
                                                "name": "ACCESS_TOKEN",
                                                "value": "${ACCESS_TOKEN}"
                                            }
                                        ],
                                        "ports": [
                                            {
                                                "name": "${APPNAME}proxy-tcp-8080",
                                                "containerPort": 8080,
                                                "protocol": "TCP"
                                            }
                                        ],
                                        "resources": {},
                                        "imagePullPolicy": "",
                                        "capabilities": {}
                                    }
                                ],
                                "restartPolicy": {}
                            }
                        },
                        "labels": {
                            "deploymentconfig": "${APPNAME}proxy"
                        }
                    }
                }
            }
        }
    ]
}
